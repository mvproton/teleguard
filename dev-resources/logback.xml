<configuration>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date %-5level [%thread] %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>


    <logger name="teleguard" level="TRACE">
        <appender-ref ref="CONSOLE"/>
    </logger>

</configuration>